{
	"name": "vscode-ruby-debugger",
	"version": "0.24.2",
	"description": "Debugger implementation for vscode-ruby",
	"main": "src/index.ts",
	"repository": "https://github.com/rubyide/vscode-ruby",
	"author": "Stafford Brunk <stafford.brunk@gmail.com>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "parcel build -t node src/main.ts",
		"compile": "tsc -p tsconfig.json",
		"clean": "mkdir -p dist && rm -rf dist/*",
		"lint": "eslint src/**/*.ts",
		"test": "nyc mocha -r ts-node/register -r source-map-support/register spec/**/*.ts",
		"watch": "tsc -w -p tsconfig.json"
	},
	"dependencies": {
		"vscode-debugadapter": "^1.35.0",
		"vscode-debugprotocol": "^1.35.0",
		"xmldom": "^0.1.27"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/xmldom": "^0.1.29",
		"@typescript-eslint/eslint-plugin": "^1.13.0",
		"@typescript-eslint/parser": "^1.13.0",
		"chai": "^4.2.0",
		"eslint": ">=6.0.1",
		"eslint-config-standard": "^13.0.1",
		"eslint-config-standard-with-typescript": "^8.0.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-node": "^9.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.0",
		"mocha": "^6.2.0",
		"nyc": "^14.1.1",
		"parcel-bundler": "^1.12.3",
		"prettier": "^1.18.2",
		"source-map-support": "^0.5.12",
		"ts-node": "^8.3.0",
		"typescript": "^3.5.3",
		"vscode-debugadapter-testsupport": "^1.35.0"
	}
}
